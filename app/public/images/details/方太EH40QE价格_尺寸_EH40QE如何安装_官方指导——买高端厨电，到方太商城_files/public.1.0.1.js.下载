    $.fn.showbig = function (obj) {
        return this.each(function () {
            var el = $(this);
            var big = el.clone().appendTo('body').hide().css({
                position: 'absolute',
                top: el.offset().top + el.height(),
                left: el.offset().left,
                border: '1px solid #E9E9E9',
                'box-shadow': '1px 1px #E9E9E9',
                width: 300
            }).addClass('biggerThanBigger');

            el.on('mouseenter', function (e) {
                big.show();
            });

            el.on('mouseleave', function (e) {
                if ($(e.target).hasClass('biggerThanBigger')) {
                } else {
                    big.hide();
                }
            });

            big.on('mouseenter', function (e) {
                big.show();
            });

            big.on('mouseleave', function (e) {
                $(this).hide();
            });

        });
    };

    $('.showbig').showbig();

    /* for flower-product */
    $('.hotsale').on('click', '.btn-fav', function () {
        var el = $(this);
        if (ISLOGIN==="0") {
            $.lgs.show('login');
            return false;
        }
        var proType = el.parents('.item-box').attr('data-type');
        var proId = el.parents('.item-box').attr('data-id');
        $.ajax({
            url: "/handle.aspx",
            data: {
                action: "InsertFav",
                _pid: proId,
                _type: proType
            },
            success: function (res) {
                if (res == "-1") {
                    alert("亲，请登录后再收藏商品！");
                } else if (res == 'success') {
                    el.addClass('on');
                } else if (res == "HavenInsert") {
                    alert("您已经收藏，无需再次收藏！");
                    el.addClass('on');
                } else {
                    alert('收藏失败');
                }
            }
        });
    });

