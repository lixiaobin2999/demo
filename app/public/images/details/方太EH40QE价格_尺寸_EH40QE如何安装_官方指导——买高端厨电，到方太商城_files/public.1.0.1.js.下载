$(function () {

    //sx,value模拟placeholder？？
    var $searchInput = $('#include_top1_txtkey');
    var searchDefault = $searchInput.val();
    $searchInput.focus(function () {
        if (searchDefault === $(this).val()) {
            $(this).val('');
        }
    }).blur(function () {
        if(!$(this).val().trim()){
            $(this).val(searchDefault);
        }
    });

    window.onload = function () {
        $searchInput.blur();
    }
    //endSX


    if (location.href.split('?')[0].indexOf('integralshop') > -1) {
        $('#include_top1_txtkey').val('');
    }

    $.fn.layAlert = function (obj, cb) {
        var that = this;
        var def = {
            title: '',
            msg: '',
            width: 100,
            height: 100,
            left: '50%',
            top: 0,
            cnt: that,
            isMask: 1,
            isBtnSubmit: '<span class="lay-btnSubmit">是</span>',
            isBtnCancel: '<span class="lay-btnCancel">否</span>',
            className: 'lay-box',
            submit: function (e) {
                e.remove();
                $('.mask').remove();
            },
            cancel: function (e) {
                e.remove();
                $('.mask').remove();
            },
            closed: function (e) {
                e.remove();
                $('.mask').remove();
            }
        };
        var st = $.extend(def, obj || {});

        if (st.isMask) {
            var mask = $('.mask').size() > 0 ? $('.mask') : $('<div class="mask"></div>').appendTo('body');
            mask.show().css({
                left: 0,
                top: 0
            }).appendTo(st.cnt);
        }

        var lay = $('.' + st.className).size() > 0 ? $('.' + st.className) : $('<div class="' + st.className + '"><div class="lay-hd">' + st.title + '</div><div class="lay-bd">' + st.msg + '</div><div class="lay-ft">' + st.isBtnSubmit + st.isBtnCancel + '</div><span class="lay-closed"></span></div>').appendTo('body');

        lay.show().appendTo(st.cnt).css({
            width: st.width,
            height: st.height,
            left: st.left,
            top: $(window).height() / 2 - st.height / 2 + $(window).scrollTop(),
            'margin-left': -st.width / 2
        });

        lay.find('.lay-btnSubmit').click(function () {
            if (st.submit && typeof st.submit == "function") {
                st.submit(lay);
            }
        });
        lay.find('.lay-btnCancel').click(function () {
            if (st.cancel && typeof st.cancel == "function") {
                st.cancel(lay);
                $('.mask').remove();//写了隐藏不隐藏再删一次
            }
        });
        lay.find('.lay-closed').click(function () {
            if (st.closed && typeof st.closed == "function") {
                st.closed(lay);
            }
        });

        if (cb && typeof cb == "function") {
            cb();
        }
        return that;
    };



