function showChoseKF() {

   
    var $choseKF = $("<div id='chooseKFFF' style='position:fixed;top:0;right:0;bottom:0;left:0;background-color:rgba(102,102,102,.3);z-index:11000'>\
                        <div class='kfff_enter' style='opacity:0;width:480px;position:absolute;left:50%;top:0%;transform:translate3d(-50%,-50%,0);-webkit-transform:translate3d(-50%,-50%,0);'>\
                            <img usemap='#kffMap' src='/images/v3.0/zxwindow.png' width='480' style='' />\
                            <a href='javascript:hideChosKF()' style='display:inline-block;position:absolute;right:0px;top:0px;width:40px;height:40px'></a>\
                            <a href='javascript:beforeKF()' style='display:inline-block;position:absolute;width:186px;height:146px;top:126px;left:34px;'></a>\
                            <a href='javascript:afterKF()' style='display:inline-block;position:absolute;width:186px;height:146px;top:126px;left:258px;'></a>\
                        </div>\
                      </div>\
                    ");
    $choseKF.find('.kfff_enter').animate({opacity:1,top:'50%'});
    $choseKF.appendTo('body');
}

$(document).on('click', '#chooseKFFF', function (e) {
    if (e.target.id === 'chooseKFFF') {
        hideChosKF();
    }
});

function hideChosKF() {
    $('#chooseKFFF').fadeOut(function () {
        $(this).remove();
    });
}

function beforeKF() {
    try {
        _BFD.UserAction("pre_sales_consulting");
    } catch (e) {

    }
    NTKF.im_openInPageChat('kf_9729_1434606405539');
    $('#chooseKFFF').remove();
}

function afterKF() {
    try {
        _BFD.UserAction("after_sales_consulting");
    } catch (e) {

    }
    NTKF.im_openInPageChat('kf_9729_1434606339202');
    $('#chooseKFFF').remove();
}

function activityKF() {
    try {
        _BFD.UserAction("events_enquiries");
    } catch (e) {

    }
    NTKF.im_openInPageChat('kf_9729_1422865742088')
}