    $(document).on('mouseenter', '.animBorder', function () {
        var el = $(this);
        if (!el.attr('data-flag')) {
            var s = $('<s class="line loc_l"></s><s class="line loc_r"></s><s class="line loc_t"></s><s class="line loc_b"></s>').appendTo(el);
            el.attr('data-flag', 1);
        }
    });

