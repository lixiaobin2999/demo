    $.fn.chklogin = function (obj) {
        location.href = api.loc + "login.aspx?refurl=" + location.href.replace('?', '%tf%').replace('&', '%ft%');
    };
    $.fn.chkCoding = function (Str) {
        if (Str === null || Str == "undefined") {
            return "";
        }
        Str = Str.replace(/^[\s]+/, "").replace(/[\s]+$/, "").replace(/\W/g, function (e) {
            if (e == "+" || e == "＋") {
                return "＋";
            } else if (e == "-" || e == "－") {
                return "-";
            } else if (e == ".") {
                return ".";
            } else if (/[\u4E00-\u9FA5\uF900-\uFA2D]|[\uFF00-\uFFEF]/.test(e)) {
                return e;
            } else if (/\s/.test(e)) {
                return e;
            } else {
                return "";
            }
        });
        if (Str === null || Str == "undefined") {
            return "";
        } else {
            return Str;
        }


    };
    
    

    function searchclick() {
        var keyword = document.getElementById("include_top1_txtkey").value;
        if (keyword === null || keyword === "") {//现在没有空的情况了，空回去搜索预定义的字符
            alert("请输入搜索内容");
            return false;
        } else {
            if (keyword.length >= 2) {
                try {
                    _BFD.UserAction("search", { "search_keywords": keyword });
                } catch (e) {

                }
               
                if (location.href.split('?')[0].indexOf('integralshop') > -1) {
                    location.href = api.loc + "integralshop.html?keywords=" + $.fn.chkCoding(keyword);
                } else {
                    location.href = api.loc + "productsearch/keywords-" + $.fn.chkCoding(keyword) + ".html";
                }
            } else {
                alert("搜索内容请大于2个字");
                return false;
            }
        }
    }



    $('.search-box span.search-icon').on('click', function () {

        searchclick();
    });
    var search = $('#include_top1_txtkey');
    var sPos = search.offset();
    var tiplay = $('<ul class="tiplay search"></ul>').appendTo('.search-border');
    tiplay.css({
        'width': search.parent().width(),
        'left': 0,
        'top': 25,
        'line-height': 30
    });
    var selectIndex = -1,
        selectVal = '',
        selectLi,
        curtxt = '',
        x;

    search.on('input', function () {
        var tmpStr = '';
        var self = $(this);
        curtxt = self.val();
        if (curtxt === '') {
            tiplay.html('').removeClass('isShow');
            return false;
        }
        x = [];
        selectIndex = -1;
        tiplay.html('');
        $.each(keyslist, function (i, v) {
            if (new RegExp(curtxt, 'i').test(v) && x.length < 10) {
                x.push(v);
                v = v.replace(new RegExp(self.val(), 'ig'), function (e) {
                    return '<b>' + e + '</b>';
                }).replace(/\s+/, '');
                //tiplay.append('<li>' + v + '</li>');
                tmpStr += '<li>' + v + '</li>';
            }
        });
        $(tmpStr).appendTo(tiplay);

        if (x.length > 0) {
            tiplay.attr('ismatch', x.length).addClass('isShow');
        } else {
            tiplay.attr('ismatch', 0).removeClass('isShow');
        }
        selectLi = tiplay.find('li');
    });
    tiplay.bind('click', 'li', function (e) {
        var self = /^li$/i.test(e.target.tagName) ? $(e.target) : $(e.target).parent();
        search.val(self.text());
        tiplay.html('').removeClass('isShow');
        search.focus();
    });
    function getnext(a, count) {
        a++;
        if (a >= count) {
            a = 0;
        }
        return a;
    }
    function getprev(a, count) {
        a--;
        if (a < 0) {
            a = count - 1;
        }
        return a;
    }
    $('body').click(function (e) {
        var self = $(this);
        if (!self.hasClass('tiplay') || !self.parents('tiplay').size() > 0) {
            tiplay.html('').removeClass('isShow');
        }
    });
    $(document).on('keydown', '#include_top1_txtkey:focus', function (e) {
        if (tiplay.attr('ismatch') && tiplay.attr('ismatch') !== 0) {
            if (e.keyCode == 38) {
                //to pgup;
                selectIndex = getprev(selectIndex, selectLi.size());
                selectLi.eq(selectIndex).addClass('on').siblings().removeClass('on');
                selectVal = x[selectIndex];
                search.val(selectVal);
            } else if (e.keyCode == 40) {
                //to pgdn;
                selectIndex = getnext(selectIndex, selectLi.size());
                selectLi.eq(selectIndex).addClass('on').siblings().removeClass('on');
                selectVal = x[selectIndex];
                search.val(selectVal);
            } else if (e.keyCode == 9) {
                //tab
                //search.val(selectVal);
                //tiplay.html('').removeClass('isShow');
            } else if (e.keyCode == 27) {
                tiplay.html('').removeClass('isShow');
                search.focus();
                alert(1);
            }
        }
        if (e.keyCode == 13 && $(this).val().replace(/^[\s]+/, '').replace(/[\s]+$/, '').length > 1) {
            //enter
            var keyword = $('.search-txt').val();
            if (location.href.split('?')[0].indexOf('integralshop') > -1) {
                location.href = api.loc + "integralshop.html?keywords=" + $.fn.chkCoding(keyword);
            } else {
                location.href = api.loc + "productsearch/keywords-" + $.fn.chkCoding(keyword) + ".html";
            }
        }
    });

    $(document).on('keydown', 'input[type="text"]:focus,input[type="password"]:focus,textarea:focus', function (e) {
        if (e.keyCode == "13") {
            var tg = $(this);
            if (tg.attr('id') == 'include_top1_txtkey' || tg.attr('id') == 'include_top1_btns') {
                $('#include_top1_btns').click();
            } else if (tg.parents(".window-containter").size() > 0 || tg.hasClass('msg-text')) {
                return true;
            } else {
                var regbtn = "";
                if ($('.reg-type').length > 0) {
                    if ($('.reg-type .on').hasClass('byphone')) {
                        regbtn = "#submit_phone";
                    } else if ($('.reg-type .on').hasClass('bymaill')) {
                        regbtn = "#submit_mail";
                    }
                }
                $("#btn_login,#btnSubmit,#btnAdd,#btn,#btn-submit" + regbtn).click();
            }
            return false;
        }
    });
    


