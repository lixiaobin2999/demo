    /* 侧滑红包 */
    //红包，json
    $.fn.redPt = function (parm, fn) {
        var defaults = {
            action: 'CouponConReceive'//接口的action(必须,默认红包);
        };
        var inParm = $.extend(defaults, parm.data || {});
        $.ajax({
            url: (parm.url ? parm.url : api.url) + parm.file,//拼接url;
            data: inParm,
            dataType: parm.dataType || 'jsonp',//返回jsonp;
            type: 'get',
            success: function (d) {//接口调用成功;
                if (d !== null && d != "undefined") {//成功获取返回数据;
                    if (typeof d == "number" && d === 0) {//未登录
                        $('.redpacket-btn').attr('data-num', 0);
                    } else if (d.length > 0) {//有可以领取的红包
                        $('.redpacket-btn').attr('data-num', 1);
                        var v = d[0];
                        var redUrl = "javascript:void(0)";
                        var red = $('<a href="' + redUrl + '" target="_blank" data-id="' + v.ID + '" data-s="' + v.TypeID + v.CodeLength + v.StartWords.substring(0, 1) + v.Number + '" class="redpacket"><p class="itemRedpacket">' + v.TypeValue + '</p><span class="btn btn-getNow">立即领取</span></a>').appendTo('.redpacket-lay .lay-bd');
                        var getMore = $('<a href="' + api.loc + 'newcenterredpacket.aspx" class="getMoreRed" target="_blank">更多红包</a>').appendTo('.redpacket-lay .lay-bd');
                    } else {//无可以领取的红包
                        $('.redpacket-btn').attr('data-num', 2);
                        var red = $('<p>已领取</p><a class="btn moreRedpacket" href="' + api.loc + 'newcenterredpacket.aspx" target="_blank">查看我的红包</a>').appendTo('.redpacket-lay .lay-bd');
                    }
                }
                if (fn && typeof (fn) == 'function') {
                    //如果有回调函数，执行回调
                    if (d === null && d == "undefind") {
                        fn();
                    } else {
                        fn(d);
                    }
                }
            }
        });

    };
    $.fn.redPt({
        url: "/",
        file: api.fn['redpacket'],
        tmp: 'redpacket',
        data: { action: api.action['redpacketList'] },
        dataType: "json",
        nd: $('.redpacket-lay .lay-bd')
    });
    //关闭弹窗
    $(document).on('click', '.lay-closed', function () {
        var el = $(this);
        el.parents('.redpacket-lay').hide();
    });
    //滑过红包按钮
    $(document).on('mouseenter', '.redpacket-btn', function () {
        var el = $(this);
        var num = $('.redpacket-btn').attr('data-num');
        if (num == 1 || num == 2) {
            el.parents('.float').find('.redpacket-lay').stop().fadeIn();
        }
    })
        .on('mouseleave', '.redpacket-btn', function () {
            var el = $(this);
            el.parents('.float').find('.redpacket-lay').stop().fadeOut();
        })
        .on('click', '.redpacket-btn >span', function () {//领取红包
            var el = $(this);
            if (location.pathname.indexOf('login.aspx') >= 0 || location.pathname.indexOf('register.aspx') >= 0) {
                el.hide();
            } else if ($('.redpacket-btn').attr('data-num') === '0') {
                $.lgs.show('login');
            } else if ($('.redpacket-btn').attr('data-num') === '1') {
                el = $(this).parents('.redpacket-btn').find('.redpacket');
                $.ajax({
                    url: "/newhandle.aspx",
                    data: {
                        _id: el.attr('data-id'),
                        _s: el.attr('data-s'),
                        action: 'ReceiveCashCode'
                    },
                    type: 'post',
                    success: function (d) {
                        //-1:领取地址不正确，请检查    0:未登录   -2:无法重复领取  -3:下手慢了，已经被领完了！ 1:领取成功
                        if (!d) {
                            return;
                        }
                        switch (d) {
                            case '1':
                                var redFly = $('<span><img src="/images/v3.0/redpackage-2.png"></span>').css({
                                    position: 'absolute',
                                    display: 'inline-block',
                                    top: el.offset().top,
                                    left: el.offset().left
                                }).appendTo('body');
                                redFly.animate({
                                    width: 0,
                                    heigth: 0,
                                    top: $('#showuser').offset().top,
                                    left: $('#showuser').offset().left
                                }, 1500, 'swing', function () {
                                    $(this).remove();
                                    alert('领取成功');
                                    $.fn.redPt({
                                        url: "/",
                                        file: api.fn['redpacket'],
                                        tmp: 'redpacket',
                                        data: { action: api.action['redpacketList'] },
                                        dataType: "json",
                                        nd: $('.redpacket-lay .lay-bd').empty()
                                    });
                                });
                                
                                break;
                            case '0': $.lgs.show('login'); break;
                            case '-1': alert('领取地址不正确，请检查'); break;
                            case '-2': alert('无法重复领取!'); break;
                            case '-3': alert('下手慢了，已经被领完了！'); break;
                        }
                    }
                });
            }
        });

    $(document).on('click', 'a.redpacket', function (e) {
        e.preventDefault();
        var el = $(this).parents('.redpacket-btn').find('.redpacket');
        if ($('.redpacket-btn').attr('data-num') == 1) {
            $.ajax({
                url: "/newhandle.aspx",
                data: {
                    _id: el.attr('data-id'),
                    _s: el.attr('data-s'),
                    action: 'ReceiveCashCode'
                },
                type: 'post',
                success: function (d) {
                    //-1:领取地址不正确，请检查    0:未登录   -2:无法重复领取  -3:下手慢了，已经被领完了！ 1:领取成功
                    if (!d) {
                        return;
                    }
                    switch (d) {
                        case '1':
                            var redFly = $('<span><img src="/images/v3.0/redpackage-2.png"></span>').css({
                                position: 'absolute',
                                display: 'inline-block',
                                top: el.offset().top,
                                left: el.offset().left
                            }).appendTo('body');
                            redFly.animate({
                                width: 0,
                                height: 0,
                                top: $('#showuser').offset().top,
                                left: $('#showuser').offset().left
                            }, 1500, 'swing', function () {
                                $(this).remove();
                                alert('领取成功');
                            });
                            $.fn.redPt({
                                url: "/",
                                file: api.fn['redpacket'],
                                tmp: 'redpacket',
                                data: { action: api.action['redpacketList'] },
                                dataType: "json",
                                nd: $('.redpacket-lay .lay-bd').empty()
                            });
                            break;
                        case '0': $.lgs.show('login'); break;
                        case '-1': alert('领取地址不正确，请检查'); break;
                        case '-2': alert('无法重复领取!'); break;
                        case '-3': alert('下手慢了，已经被领完了！'); break;
                    }
                }
            });
        } else if ($('.redpacket-btn').attr('data-num') === '0') {
            $.lgs.show('login');
        } else if (location.pathname.indexOf('login.aspx') >= 0 || location.pathname.indexOf('register.aspx') >= 0) {
            $('.redpacket-btn').hide();
        }
    });

