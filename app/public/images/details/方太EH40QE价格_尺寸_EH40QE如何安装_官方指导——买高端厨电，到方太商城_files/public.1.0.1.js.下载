    /* 公告 */

    var gg = $('.gonggao p');
    function ggr2l(el) {
        if (el.size() < 1) {
            return false;
        }
        var speed = Math.round(20000 * (50 + el.offset().left / $(window).width() * 100) / 150);
        el.stop().animate({ left: '-50%' }, speed, 'linear', function () {
            el.css({ left: '100%' });
            ggr2l(el);
        });
    }
    ggr2l(gg);

    gg.on('mouseenter', function () {
        $(this).stop();
    }).on('mouseleave', function () {
        ggr2l($(this));
    });

